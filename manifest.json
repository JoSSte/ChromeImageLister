{
    "name": "ChrListImages",
    "description": "List images on page",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "tabs",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "/images/listimg16.png",
          "32": "/images/listimg32.png",
          "48": "/images/listimg48.png",
          "128": "/images/listimg128.png"
        }
    },
    "options_page": "options.html",
    "icons": {
        "16": "/images/listimg16.png",
        "32": "/images/listimg32.png",
        "48": "/images/listimg48.png",
        "128": "/images/listimg128.png"
    }
}